{
  "name": "mtgatool-web",
  "version": "2.0.0",
  "description": "Website for MTG Arena Tool",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack",
    "format": "prettier --write 'packages/**/*.js'",
    "eslint": "eslint src/ --ext ts --ext tsx",
    "eslint:fix": "eslint src/ --ext ts --ext tsx --fix",
    "test": "npm run eslint && jest --watchAll --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleFileExtensions": [
      "css",
      "scss",
      "js",
      "json",
      "jsx"
    ]
  },
  "author": "Manuel-777",
  "license": "GPL",
  "private": false,
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "dependencies": {
    "@react-spring/parallax": "^9.0.0-rc.3",
    "core-js": "3.4.5",
    "crypto": "^1.0.1",
    "lodash": "^4.17.15",
    "prop-types": "15.7.2",
    "raw-loader": "^4.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-google-recaptcha": "^2.1.0",
    "react-hot-loader": "4.12.18",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "react-spring": "^9.0.0-rc.3",
    "react-use-gesture": "^7.0.15",
    "yaml": "^1.10.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-do-expressions": "^7.10.4",
    "@babel/plugin-transform-destructuring": "^7.10.4",
    "@babel/plugin-transform-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "7.7.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "7.7.4",
    "@babel/runtime": "7.7.4",
    "@teamsupercell/typings-for-css-modules-loader": "^2.2.0",
    "@types/bytebuffer": "^5.0.41",
    "@types/lodash": "^4.14.157",
    "@types/long": "^4.0.1",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "3.3.4",
    "babel-preset-react-optimize": "1.0.1",
    "browserslist": "4.8.0",
    "clean-webpack-plugin": "3.0.0",
    "connect-history-api-fallback": "1.6.0",
    "cross-env": "6.0.3",
    "css-loader": "3.2.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint": "6.7.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.21.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
